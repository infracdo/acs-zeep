lbarDense\"\n        color=\"secondary\"\n        dark\n      >\n        <v-toolbar-title>\n          {{ $t('settings.title') }}\n        </v-toolbar-title>\n        <v-tooltip left>\n          <template v-slot:activator=\"{on}\">\n            <v-btn\n              absolute\n              dark\n              fab\n              bottom\n              right\n              small\n              color=\"error\"\n              v-on=\"on\"\n              @click.stop=\"setDefaultSettingsPanel\"\n            >\n              <v-icon>mdi-refresh</v-icon>\n            </v-btn>\n          </template>\n          <span>{{ $t('settings.default') }}</span>\n        </v-tooltip>\n      </v-app-bar>\n\n      <v-subheader class=\"layout-fab__header\">\n        {{ $t('settings.version') }}: {{ version }}\n      </v-subheader>\n      <v-divider />\n\n      <v-expansion-panels accordion>\n        <v-expansion-panel>\n          <v-expansion-panel-header class=\"layout-fab__header\">\n            {{ $t('settings.position') }}\n          </v-expansion-panel-header>\n          <v-expansion-panel-content>\n            <v-switch\n              v-for=\"(toggle, i) in switches\"\n              :key=\"i\"\n              dense\n              hide-details\n              color=\"secondary\"\n              class=\"layout-fab__switch\"\n              :disabled=\"toggle.value === null\"\n              :input-value=\"toggle.value\"\n              :label=\"toggle.label\"\n              @change=\"toggle.change\"\n            />\n          </v-expansion-panel-content>\n        </v-expansion-panel>\n\n        <v-expansion-panel>\n          <v-expansion-panel-header class=\"layout-fab__header\">\n            {{ $t('settings.theme') }}\n          </v-expansion-panel-header>\n          <v-expansion-panel-content>\n            <app-theme cols=\"6\" />\n          </v-expansion-panel-content>\n        </v-expansion-panel>\n      </v-expansion-panels>\n    </v-navigation-drawer>\n  </v-main>\n</template>\n\n<script