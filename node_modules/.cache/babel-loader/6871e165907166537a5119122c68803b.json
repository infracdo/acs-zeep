() {\n      this.$store.dispatch('NavbarDenseToggle');\n    },\n    toggleNavbarLogo() {\n      this.$store.dispatch('NavbarLogoToggle');\n    },\n    toggleSettingsPanelBtn() {\n      this.$store.dispatch('SettingsPanelBtnToggle');\n    },\n    toggleFooterToggle() {\n      this.$store.dispatch('FooterToggle');\n    },\n    setDefaultSettingsPanel() {\n      this.$store.dispatch('SettingsPanelDefault');\n    },\n  },\n};\n</script>\n\n<style>\n  .layout-fab {\n    position: absolute;\n  }\n  .layout-fab__btn {\n    margin-top: 4px; /* fixme: vertical align */\n    top: 50% !important;\n    right: 0 !important;\n    border-radius: 0 !important;\n  }\n  .layout-fab__switch {\n    margin-top: 4px;\n  }\n  .layout-fab__header {\n    padding: 0 12px 0 12px !important;\n  }\n  .v-expansion-panel-content__wrap {\n    padding:0 12px 12px;\n  }\n</style>\n"],"mappings":";;AA6FA,SAAAA,UAAA;AACA,OAAAC,QAAA;AAEA;EACAC,IAAA;EACAC,UAAA;IAAAF,QAAA,EAAAA;EAAA;EACAG,IAAA,WAAAA,KAAA;IAAA;EAAA;EACAC,QAAA,EAAAC,aAAA,CAAAA,aAAA,KACAN,UAAA,EACA,WACA,aACA,oBACA,qBACA,gBACA,eACA,cACA,aACA;IACAO,QAAA,WAAAA,SAAA;MACA,QACA;QACAC,KAAA,OAAAC,YAAA;QACAC,KAAA,KAAAC,MAAA,MAAAC,EAAA,iCAAAD,MAAA,MAAAF,YAAA;QACAI,MAAA,OAAAC;MACA,GACA;QACAN,KAAA,OAAAO,WAAA;QACAL,KAAA,KAAAC,MAAA,MAAAC,EAAA,gCAAAD,MAAA,MAAAI,WAAA;QACAF,MAAA,OAAAG;MACA,GACA;QACAR,KAAA,OAAAS,UAAA;QACAP,KAAA,KAAAC,MAAA,MAAAC,EAAA,+BAAAD,MAAA,MAAAM,UAAA;QACAJ,MAAA,OAAAK;MACA,GACA;QACAV,KAAA,OAAAW,gBAAA;QACAT,KAAA,KAAAC,MAAA,MAAAC,EAAA,gCAAAD,MAAA,MAAAQ,gBAAA;QACAN,MAAA,OAAAO;MACA,GACA;QACAZ,KAAA,OAAAa,UAAA;QACAX,KAAA,KAAAC,MAAA,MAAAC,EAAA;QACAC,MAAA,OAAAS;MACA,GACA;QACAd,KAAA,OAAAe,SAAA;QACAb,KAAA,KAAAC,MAAA,MAAAC,EAAA;QACAC,MAAA,OAAAW;MACA,EACA;IACA;EAAA,EACA;EACAC,OAAA;IACAC,kBAAA,WAAAA,mBAAAC,KAAA;MACA,KAAAC,MAAA,CAAAC,QAAA;QAAAF,KAAA,EAAAA;MAAA;IACA;IACAG,mBAAA,WAAAA,oBAAA;MACA,KAAAC,QAAA,CAAAC,IAAA;MACA,KAAAJ,MAAA,CAAAC,QAAA;IACA;IACAL,eAAA,WAAAA,gBAAA;MACA,KAAAI,MAAA,CAAAC,QAAA;IACA;IACAf,kBAAA,WAAAA,mBAAA;MACA,KAAAc,MAAA,CAAAC,QAAA;