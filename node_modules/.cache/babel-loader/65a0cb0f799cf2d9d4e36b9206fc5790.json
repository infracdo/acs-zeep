en}`);\n  let user = {};\n  try {\n    if (!token) {\n      throw new Error('Invalid token');\n    }\n\n    if (token === userAdmin.token) {\n      user = userAdmin;\n    } else {\n      user = userEditor;\n    }\n  } catch (err) {\n    console.warn(`[getUserInfo] ${err}`);\n  }\n  return { user };\n};\n"],"mappings":";;;AAAA;AACA;AACA;AACA,SAASA,SAAS,EAAEC,UAAU,QAAQ,YAAY;;AAElD;AACA;AACA;AACA;AACA;AACA;AACA,OAAO,IAAMC,YAAY;EAAA,IAAAC,IAAA,GAAAC,iBAAA,cAAAC,mBAAA,GAAAC,IAAA,CAAG,SAAAC,QAAOC,KAAK,EAAEC,QAAQ;IAAA,IAAAC,IAAA;IAAA,OAAAL,mBAAA,GAAAM,IAAA,UAAAC,SAAAC,QAAA;MAAA,kBAAAA,QAAA,CAAAC,IAAA,GAAAD,QAAA,CAAAE,IAAA;QAAA;UAChDC,OAAO,CAACC,GAAG,yBAAAC,MAAA,CAAyBV,KAAK,CAAE,CAAC;UACxCE,IAAI,GAAG,CAAC,CAAC;UAAAG,QAAA,CAAAC,IAAA;UAEX,IAAIb,UAAU,CAACO,KAAK,KAAKA,KAAK,IAAIP,UAAU,CAACQ,QAAQ,KAAKA,QAAQ,EAAE;YAClEC,IAAI,GAAGT,UAAU;UACnB,CAAC,MAAM,IAAID,SAAS,CAACQ,KAAK,KAAKA,KAAK,IAAIR,SAAS,CAACS,QAAQ,KAAKA,QAAQ,EAAE;YACvEC,IAAI,GAAGV,SAAS;UAClB;UAAC,MACG,CAACU,IAAI,IAAI,CAACA,IAAI,CAACS,KAAK;YAAAN,QAAA,CAAAE,IAAA;YAAA;UAAA;UAAA,MAChB,IAAIK,KAAK,CAAC,mBAAmB,CAAC;QAAA;UAAAP,QAAA,CAAAE,IAAA;UAAA;QAAA;UAAAF,QAAA,CAAAC,IAAA;UAAAD,QAAA,CAAAQ,EAAA,GAAAR,QAAA;UAGtCG,OAAO,CAACM,IAAI,mBAAAJ,MAAA,CAAAL,QAAA,CAAAQ,EAAA,CAAwB,CAAC;QAAC;UAAA,OAAAR,QAAA,CAAAU,MAAA,WAEjC;YAAEb,IAAI,EAAJA;UAAK,CAAC;QAAA;QAAA;UAAA,OAAAG,QAAA,CAAAW,IAAA;MAAA;IAAA,GAAAjB,OAAA;EAAA,CAChB;EAAA,gBAhBYL,YAAYA,CAAAuB,EAAA,EAAAC,GAAA;IAAA,OAAAvB,IAAA,CAAAwB,KAAA,OAAAC,SAAA;EAAA;AAAA,GAgBxB;;AAED;AACA;AACA;AACA;AACA;AACA,OAAO,IAAMC,WAAW;EAAA,IAAAC,KAAA,GAAA1B,iBAAA,cAAAC,mBAAA,GAAAC,IAAA,CAAG,SAAAyB,SAAOZ,KAAK;IAAA,IAAAT,IAAA;IAAA,OAAAL,mBAAA,GAAAM,IAAA,UAAAqB,UAAAC,SAAA;MAAA,kBAAAA,SAAA,CAAAnB,IAAA,GAAAmB,SAAA,CAAAlB,IAAA;QAAA;UACrCC,OAAO,CAACC,GAAG,wBAAAC,MAAA,CAAwBC,KAAK,CAAE,CAAC;UACvCT,IAAI,GAAG,CAAC,CAAC;UAAAuB,SAAA,CAAAnB,IAAA;UAAA,IAENK,KAAK;YAAAc,SAAA,CAAAlB,IAAA;YAAA;UAAA;UAAA,MACF,IAAIK,KAAK,CAAC,eAAe,CAAC;QAAA;UAGlC,IAAID,KAAK,KAAKnB,SAAS,CAACmB,KAAK,EAAE;YAC7BT,IAAI,GAAGV,SAAS;UAClB,CAAC,MAAM;YACLU,IAAI,GA